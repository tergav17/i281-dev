01/AC: 9200     	LOADF	A,[C]
01/AD: 3400     	LOADI	B,0
01/AE: A480     	STORE	[DBANK],B
01/AF: A06B     	STORE	[DFT_USR],A
01/B0: FF08     	JUMP	EXIT
01/B2:          
