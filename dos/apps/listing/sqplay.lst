01/C0: A080     	STORE	[DBANK],A
01/C2:          	
01/C1:          	; CHECK ARG COUNT
01/C1: 8000     	LOAD	A,[ARGC]
01/C2: 7001     	SUBI	A,1
01/C3: F7F1     	BRNZ	ARGBAD
01/C5:          	
