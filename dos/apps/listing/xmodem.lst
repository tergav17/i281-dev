01/80:          	; START BY PROCESSING THE ARGUMENTS
01/80: 3000     START:	LOADI	A,0
01/81: A080     	STORE	[DBANK],A
01/83:          	
01/82:          	; RESET ARG STATE
01/82: A003     	STORE	[SFLAG],A
01/83: A004     	STORE	[TFLAG],A
01/84: A000     	STORE	[ARGC],A
01/86:          	
