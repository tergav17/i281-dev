00/00:          ; BIOS.S
01/00:          ; STANDARD BIOS FOR COMPACT FLASH BOOT
01/00:          ; PLUS STANDARD CALLS
01/00:          
01/00:          ; DEFINES
01/00:          DBANK	= 0X80		; DATA BANK ADDRESS 
01/00:          CF	= 0XA0		; COMPACT FLASH BASE ADDRESS
01/00:          CF_DATA	= CF+0X00	; CF DATA
01/00:          CF_ERR	= CF+0X01	; CF ERROR
01/00:          CF_FEAT	= CF+0x01	; CF FEATURES
01/00:          CF_CNT	= CF+0X02	; CF SECTOR COUNT
01/00:          CF_LBA0	= CF+0X03	; CF LBA BITS 0-7
01/00:          CF_LBA1	= CF+0X04	; CF LBA BITS 8-15
01/00:          CF_LBA2	= CF+0X05	; CF LBA BITS 16-23
01/00:          CF_LBA3	= CF+0X06	; CF LBA BITS 24-27
01/00:          CF_STAT	= CF+0X07	; CF STATUS
01/00:          CF_CMD	= CF+0X07	; CF COMMAND
01/00:          
01/00:          CF_8BIT	= 0X01		; 8 BIT MODE
01/00:          CF_DCAC	= 0X82		; DISABLE CACHE
01/00:          
01/00:          CF_READ	= 0X20		; READ COMMAND
01/00:          CF_SETF	= 0XEF		; SET FEATURE COMMAND
01/00:          
01/00:          .BANK 0
00/00:          .TEXT
00/00:          
00/00:          	; BIOS CALL JUMP TABLE
00/00: FF05     	JUMP	BOOT	; 0: BOOT OPERATION
00/01: FF34     	JUMP	INDIR	; 1: INDIRECT JUMP
00/02: FF35     	JUMP	IRET	; 2: INDIRECT RETURN
00/03: FF38     	JUMP	SYSCALL	; 3: SYSTEM CALL (JUMP TO BANK 0 ADDRESS 0X80, SAVE REGISTER )
00/04: FF38     	JUMP	SYSJUMP	; 4: SYSTEM JUMP (JUMP TO BANK 0 ADDRESS 0X80, DO NOT SAVE C)
00/05: FF3A     	JUMP	PRGM	; 5: PROGRAM FROM DMEM
00/07:          	
00/06:          	
00/06:          	
00/06:          	; KICK OFF THE BOOT SEQUENCE
00/06:          	; START BY SETTING THE CARD TO 8-BIT MODE
00/06:          	; WE MUST WAIT FOR THE CF TO BE READY BEFORE SENDING A COMMAND
00/06: 3808     BOOT:	LOADI	C,@+2
00/07: FF19     	JUMP	CFWAIT
00/08: 3001     	LOADI	A,CF_8BIT
00/09: A0A1     	STORE	[CF_FEAT],A
00/0A: 30EF     	LOADI	A,CF_SETF
00/0B: A0A7     	STORE	[CF_CMD],A
00/0D:          	
00/0C:          	; NOW DISABLE THE CACHE
00/0C: 380E     	LOADI	C,@+2
00/0D: FF13     	JUMP	CFWAIT
00/0E: 3082     	LOADI	A,CF_DCAC
00/0F: A0A1     	STORE	[CF_FEAT],A
00/10: 30EF     	LOADI	A,CF_SETF
00/11: A0A7     	STORE	[CF_CMD],A
00/13:          	
00/12:          	; PUNCH IN THE ADDRESS OF THE MBR
00/12:          	; (MASTER BOOT RECORD)
00/12: 3400     	LOADI	B,0
00/13: A4A3     	STORE	[CF_LBA0],B
00/14: A4A4     	STORE	[CF_LBA1],B
00/15: A4A5     	STORE	[CF_LBA2],B
00/17:          	
00/16:          	; THE TOP 3 BITS OF LBA3 MUST BE '1' TO INDICATE LBA MODE
00/16: 30E0     	LOADI	A,0XE0
00/17: A0A6     	STORE	[CF_LBA3],A
00/19:          	
00/18:          	; PUT A '1' IN THE SECTOR COUNT REGISTER
00/18: 3001     	LOADI	A,0X01
00/19: A0A2     	STORE	[CF_CNT],A
00/1B:          	
00/1A:          	; SINCE A = 1, LETS SET THE DESTINATION BANKS TOO
00/1A: 0000     	BANK	A
00/1B: A080     	STORE	[DBANK],A
00/1D:          	
00/1C:          	; ISSUE A READ COMMAND!
00/1C: 381E     	LOADI	C,@+2
00/1D: FF03     	JUMP	CFWAIT
00/1E: 3020     	LOADI	A,CF_READ
00/1F: A0A7     	STORE	[CF_CMD],A
00/21:          	
00/20:          	; ...AND WAIT FOR IT TO FINISH
00/20: 3827     	LOADI	C,CFBOOT
00/22:          
00/21:          	; WAIT FOR THE CF CARD TO BECOME READY
00/21:          	; FOR THIS TO HAPPEN, THE BUSY FLAG MUST BE 0
00/21:          	; AND THE READY FLAG MUST BE 1
00/21:          	; IF A CF CARD ISN'T PRESENT, THIS ROUTINE HANGS
00/21:          	; BUT WE DON'T CARE
00/21:          	; USES: A
00/21: 80A7     CFWAIT:	LOAD	A,[CF_STAT]
00/23:          
00/22:          	; CHECK BIT 7 (BUSY FLAG)
00/22: 4000     	SHIFTL	A
00/24:          	
00/23:          	; GO BACK TO START BUSY
00/23: F0FD     	BRC	CFWAIT
00/25:          	
00/24:          	; CHECK BIT 6 (READY FLAG)
00/24: 4000     	SHIFTL	A
00/26:          	
00/25:          	; GO BACK TO START IF NOT READY
00/25: F1FB     	BRNC	CFWAIT
00/27:          	
00/26:          	; RETURN FROM FUNCTION CALL
00/26: FED9     	JUMPR	C
00/28:          	
00/27:          	; NOW THE CARD SHOULD BE READY TO SEND BYTES
00/27:          	; WE ARE JUST GOING TO GO OUT ON A LIMB AND
00/27:          	; ASSUME THAT WHATEVER MASTER BOOT RECORD
00/27:          	; WE HAVE BEEN GIVEN IS VALID
00/27:          	; NO USE IN WASTING PRECIOUS INSTRUCTION MEMORY HERE
00/27:          	; SKIP THE FIRST 128 BYTES OF MEMORY
00/27:          	; B SHOULD STILL BE 0 FROM LOADING THE MBR ADDRESS
00/27: 80A0     CFBOOT:	LOAD	A,[CF_DATA]
00/28: 5401     	ADDI	B,1
00/29: F5FD     	BRNN	CFBOOT
00/2B:          	
00/2A:          	; NOW WE UNPACK THE DATA SEGMENT
00/2A: 3400     	LOADI	B,0
00/2B: 80A0     0:	LOAD	A,[CF_DATA]
00/2C: B100     	STOREF	[B],A
00/2D: 5401     	ADDI	B,1
00/2E: F5FC     	BRNN	0B
00/30:          	
00/2F:          	; FINALLY, WE DO THE INSTRUCTION SEGMENT
00/2F: 80A0     1:	LOAD	A,[CF_DATA]
00/30: 1400     	CACHE	A
00/31: 80A0     	LOAD	A,[CF_DATA]
00/32: 1600     	WRITE	[B],A
00/33: 5401     	ADDI	B,1
00/34: F4FA     	BRN	1B
00/36:          	
00/35:          	; NOW WE JUMP TO THE LOADED PROGRAM AND HOPE FOR THE BEST!
00/35: FF4A     	JUMP	0X80
00/37:          	
00/36:          	; INDIRECT JUMP
00/36:          	; JUMPS TO A SPECIFIC ADDRESS ON A SPECIFIC BANK
00/36:          	; B = DESTINATION ISR BANK
00/36:          	; C = DESTINATION ADDRESS
00/36:          	; USES: NONE
00/36: 0400     INDIR:	BANK	B
00/37: FEC8     	JUMPR	C
00/39:          	
00/38:          	; INDIRECT RETURN
00/38:          	; JUMPS TO AN BANK / ADDRESS PAIR FOUND IN THE STACK
00/38:          	; [D] = RETURN ADDRESS
00/38:          	; [D+1] = RETURN ISR BANK
00/38:          	; USES: C
00/38: 9B01     IRET:	LOADF	C,[D+1]
00/39: 0800     	BANK	C
00/3A: 9B00     	LOADF	C,[D]
00/3B: FEC4     	JUMPR	C
00/3D:          	
00/3C:          	; SYSTEM CALL
00/3C:          	; JUMPS TO ADDRESS 0X80 ON ISR BANK 0
00/3C:          	; USES: C
00/3C: BB00     SYSCALL:STOREF	[D],C
00/3D: 3800     SYSJUMP:LOADI	C,0
00/3E: 0800     	BANK	C
00/3F: FF40     	JUMP	0X80
00/41:          
00/40:          	; THIS ROUTINE WILL TAKE 2 PAGES FROM DATA MEMORY
00/40:          	; AND WRITE THEM INTO A BANK OF INSTRUCTION MEMORY
00/40:          	; RETURNS WITH DATA BANK ON 0
00/40:          	; A = DESTINATION ISR BANK
00/40:          	; B = SOURCE DATA BANK
00/40:          	; C = RETURN ADDRESS
00/40:          	; D = RETURN ISR BANK
00/40:          	; USES: BYTE 0X7E-0X7F OF BANK 0, A, B, C, D
00/40: 0000     PRGM:	BANK	A
00/41: 3000     	LOADI	A,0
00/42: A080     	STORE	[DBANK],A
00/43: A87E     	STORE	[0X7E],C
00/44: AC7F     	STORE	[0X7F],D
00/46:          	
00/45:          	; COPY OVER THE DATA
00/45:          	; CRAZY REGISTER SHIT
00/45: 3C80     	LOADI	D,0X80
00/46: A480     0:	STORE	[DBANK],B
00/47: 3800     	LOADI	C,0
00/48: 9200     1:	LOADF	A,[C]
00/49: 1400     	CACHE	A
00/4A: 9201     	LOADF	A,[C+1]
00/4B: C900     	SHIFTR	C
00/4C: 4B00     	ADD	C,D
00/4D: 1A00     	WRITE	[C],A
00/4E: 6B00     	SUB	C,D
00/4F: 4A00     	SHIFTL	C
00/50: 5802     	ADDI	C,2
00/51: F5F6     	BRNN	1B
00/53:          	
00/52:          	; WE REACHED THE END OF A DATA BANK
00/52:          	; MOVE ON TO THE NEXT?
00/52: 5401     	ADDI	B,1
00/53: 5C40     	ADDI	D,0X40
00/54: F4F1     	BRN	0B
00/56:          	
00/55:          	; NOPE, WE ARE DONE
00/55: 3000     	LOADI	A,0
00/56: A080     	STORE	[DBANK],A
00/57: 887E     	LOAD	C,[0X7E]
00/58: 8C7F     	LOAD	D,[0X7F]
00/59: 0C00     	BANK	D
00/5A: FEA5     	JUMPR	C
00/5C:          
00/5B:          	; HALT AT THE END OF THE ROM
00/5B:          .ORG	0x7F
